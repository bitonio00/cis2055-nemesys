@model NemesysUser 
@{
    ViewData["Title"] = "AccountParameter";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>AccountParameter</h1>

<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="FirstName" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="FirstName" class="form-control">
            <span asp-validation-for="FirstName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="LastName" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="LastName" class="form-control">
            <span asp-validation-for="LastName" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <label asp-for="PhoneNumber" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="PhoneNumber" class="form-control">
                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="LockoutEnabled" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="Email" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="AccessFailedCount" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="LockoutEnabled" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="ConcurrencyStamp" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="EmailConfirmed" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="Reports" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="Investigations" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="EmailConfirmed" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="PasswordHash" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="LockoutEnd" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="NemesysUserVotes" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="UserName" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="NormalizedUserName" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="SecurityStamp" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="TwoFactorEnabled" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="NormalizedEmail" />
        </div>
        <div class="form-group row">
            <input type="hidden" asp-for="Id" />
        </div>
        <div asp-validation-summary="All" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-primary">Update</button>
                <a href="/Home/Index" class="btn btn-primary">Cancel</a>
            </div>
        </div>
</form>